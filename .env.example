# Storage provider: local | aws | cloudflare
STORAGE_PROVIDER=local

# Local storage
# No configuration needed; files saved under public/icons

STORAGE_BUCKET=
STORAGE_PUBLIC_BASE_URL=

# AWS S3
AWS_REGION=
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_ENDPOINT=

# Cloudflare R2 (S3-compatible)
CF_R2_ENDPOINT=
CF_R2_ACCESS_KEY_ID=
CF_R2_SECRET_ACCESS_KEY=

# Icon provider config
ICON_SIZE=64

# Optional: object key prefix inside bucket/folder
STORAGE_KEY_PREFIX=

# Default fallback icon URL
DEFAULT_ICON_URL=/icons/default.svg

VITE_ICON_API_URL=http://localhost:8787
VITE_BASE=/
VITE_USE_HASH_ROUTER=false
ICON_API_PORT=8787
